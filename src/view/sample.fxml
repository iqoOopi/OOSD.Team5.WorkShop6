<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<ScrollPane fitToHeight="true" fitToWidth="true" minViewportHeight="600.0" minViewportWidth="800.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="controller.Controller">
   <content>
      <BorderPane fx:id="bpMenuBar" minHeight="600.0" minWidth="800.0">
         <left>
            <ToolBar minWidth="170.0" orientation="VERTICAL" prefHeight="400.0" BorderPane.alignment="CENTER">
               <items>
                  <Button fx:id="btnProdManag" mnemonicParsing="false" onAction="#gotoProdMng" prefWidth="160.0" text="Product Management">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnPackageManagement" mnemonicParsing="false" onAction="#goToPackageManagement" prefWidth="160.0" text="Package Management">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnBookingManagement" mnemonicParsing="false" prefWidth="160.0" text="Booking Management">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnExit" mnemonicParsing="false" onAction="#goToExit" prefWidth="160.0" text="Exit">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
               </items>
            </ToolBar>
         </left>
      </BorderPane>
   </content>
</ScrollPane>
